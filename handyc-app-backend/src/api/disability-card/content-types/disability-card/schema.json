{
  "kind": "collectionType",
  "collectionName": "disability_cards",
  "info": {
    "singularName": "disability-card",
    "pluralName": "disability-cards",
    "displayName": "Disability Card ",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Number": {
      "type": "string",
      "required": true
    },
    "Proof": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "files",
        "images"
      ]
    },
    "reviewStatus": {
      "type": "enumeration",
      "enum": [
        "unreviewed",
        "approved",
        "rejected"
      ],
      "default": "unreviewed ",
      "required": true
    },
    "expiry": {
      "type": "date",
      "required": true
    },
    "issuingDate": {
      "type": "date",
      "required": true
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "mappedBy": "disability_card"
    }
  }
}
